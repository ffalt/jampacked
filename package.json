    "releaselog": "conventional-changelog -p angular -r 2 -o RELEASELOG.md",
    "logos": "react-native generate-bootsplash src/assets/images/logo.png --background-color=0b1821 --logo-width=100 --assets-path=assets",
    "clean:yarn": "rm -Rf node_modules && yarn install",
    "clean:project": "react-native-clean-project",
    "clean:gradle": "cd android && rm -Rf .gradle && rm -Rf app/build && ./gradlew clean",
    "clean": "yarn clean:yarn && yarn clean:gradle",
    "reinstall:player": "yarn remove react-native-track-player && yarn add ../react-native-track-player",
    "codegen": "graphql-codegen --config codegen.ts -v",
    "codegen:add-header": "sed -i '' '1s/^/\\/\\/ @generated\\n\\/\\/ This file was automatically generated and should not be edited.\\n\\n/'"
