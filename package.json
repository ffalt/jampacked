    "react-native-svg": "12.1.1",
    "react-native-swift": "1.2.3",
    "react-native-tab-view": "3.1.1",
    "react-native-track-player": "ffalt/react-native-track-player",
    "react-native-vector-icons": "8.1.0",
    "waveform-data": "4.1.1"
