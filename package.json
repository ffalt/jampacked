{
  "name": "jam",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android && react-native log-android",
    "android:release": "react-native run-android --variant=release && react-native log-android",
    "android:build:prepare": "jetify",
    "android:build:release:apk": "cd android && ./gradlew assembleRelease",
    "android:build:release:aab": "cd android && ./gradlew bundleRelease",
    "ios": "react-native run-ios && react-native log-ios",
    "start": "react-native start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "jest",
    "release": "standard-version",
    "changelog:start": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1",
    "releaselog": "conventional-changelog -p angular -r 2 -o RELEASELOG.md",
    "logos": "react-native generate-bootsplash src/assets/images/logo.png --background-color=0b1821 --logo-width=100 --assets-path=assets",
    "clean:gradle": "cd android && rm -Rf .gradle && rm -Rf app/build && ./gradlew clean",
    "clean": "rm -Rf node_modules && yarn install && yarn clean:gradle",
    "build:query-types": "apollo codegen:generate --localSchemaFile=schema.graphql --no-addTypename --target=typescript --includes=src/**/*.ts --tagName=gql --globalTypesFile=src/services/queries/types/graphql-types.ts types"
  },
  "resolutions": {
    "d3-array": "2.3.3",
    "graphql": "15.5.1"
  },
  "dependencies": {
    "@apollo/react-hooks": "4.0.0",
