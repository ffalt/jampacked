{
  "extends": "@tsconfig/react-native/tsconfig.json",
  /* Recommended React Native TSConfig base */
  "compilerOptions": {
    "jsx": "react",
    "skipLibCheck": true
    /* Skip type checking all .d.ts files. */
  },
  "exclude": [
    "node_modules/*",
    "local/",
    "__tests__/**/*-test.ts",
    "index.js",
    "wdyr.js",
    "node_modules/",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js",
    "jest.transformer.js"
  ]
}
