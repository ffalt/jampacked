{
  "extends": "./tsconfig.json",
  "include": [
    "./App.tsx",
    "./index.js",
    "./jest.transformer.js",
    "./jest.config.js",
    "./jest.setup.js",
    "./babel.config.js",
    "./metro.config.js",
    "./src/**/*.js",
    "./src/**/*.ts",
    "./src/**/*.tsx",
    "./__tests__/**/*.ts",
    "./__tests__/**/*.tsx"
  ],
  "exclude": [
    "node_modules/",
    "local/"
  ],
  "compilerOptions": {
    "allowSyntheticDefaultImports": true
  }
}
